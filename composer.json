{
    "name": "rezzza/mail-extra-bundle",
    "description": "Mail tools for Swift_Mailer.",
    "keywords": ["mail", "tools", "symfony", "bundle"],
    "type": "symfony-bundle",
    "homepage": "https://github.com/rezzza/MailExtraBundle",
    "license": "MIT",
    "authors": [{
        "name": "Community contributors",
        "homepage": "https://github.com/rezzza/MailExtraBundle/contributors"
    }],
    "require": {
        "php": ">=5.3.2",
        "symfony/framework-bundle": "2.*",
        "symfony/swiftmailer-bundle": "dev-feature/mailers as 2.2.1"
    },
    "repositories": [
        {
            "package": {
                "name": "symfony/swiftmailer-bundle"
            },
            "type": "git",
            "url": "https://github.com/rezzza/SwiftmailerBundle.git"
        }
    ],
    "minimum-stability": "dev",
    "autoload":     {
        "psr-0": { "Rezzza\\MailExtraBundle": "" }
    },
    "target-dir": "Rezzza/MailExtraBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
